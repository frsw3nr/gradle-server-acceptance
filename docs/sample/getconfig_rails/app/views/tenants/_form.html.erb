<%= simple_form_for @tenant, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :tenant_name %>

  <%= f.button :submit, :class => 'btn-primary' %>
  <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                tenants_path, :class => 'btn btn-default' %>
<% end %>
