/dev/mapper/vg_alpaca2-lv_root /                       ext4    defaults        1 1
UUID=fd64fb3c-c392-42b7-a4b0-9e58b155f144 /boot                   ext4    defaults        1 2
/dev/mapper/vg_alpaca2-lv_swap swap                    swap    defaults        0 0
tmpfs                   /dev/shm                tmpfs   defaults        0 0
devpts                  /dev/pts                devpts  gid=5,mode=620  0 0
sysfs                   /sys                    sysfs   defaults        0 0
proc                    /proc                   proc    defaults        0 0