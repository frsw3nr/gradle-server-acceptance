/dev/mapper/vg_ostrich-lv_root /                       ext4    defaults        1 1
UUID=a8c2e1f6-fd5e-4e4a-b44e-867e411ca318 /boot                   ext4    defaults        1 2
/dev/mapper/vg_ostrich-lv_swap swap                    swap    defaults        0 0
tmpfs                   /dev/shm                tmpfs   defaults        0 0
devpts                  /dev/pts                devpts  gid=5,mode=620  0 0
sysfs                   /sys                    sysfs   defaults        0 0
proc                    /proc                   proc    defaults        0 0